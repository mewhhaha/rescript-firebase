{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "rescript-app",
  "reason": {
    "react-jsx": 3
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "external-stdlib": "@rescript/std",
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": ["@rescript/react", "reason-uuid"],
  "bs-dev-dependencies": [],
  "bsc-flags": ["-open Belt"],
  "ppx-flags": [],
  "refmt": 3
}
